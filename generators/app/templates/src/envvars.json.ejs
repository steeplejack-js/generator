{
  "logging": {
    "name": "<%- envvarPrefix %>LOGGING_STREAMS_NAME",
    "streams": {
      "stdout": {
        "active": "<%- envvarPrefix %>LOGGING_STREAMS_STDOUT_ACTIVE",
        "level": "<%- envvarPrefix %>LOGGING_STREAMS_STDOUT_LEVEL"
      }
    }
  },
  "server": {<% if (server === 'express') { %>
    "displayErrorDetail": "<%- envvarPrefix %>SERVER_DISPLAY_ERROR_DETAIL",<% } %>
    "port": "<%- envvarPrefix %>SERVER_PORT"
  }
}
